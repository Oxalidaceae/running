{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run build --workspace=@running/frontend",
  "outputDirectory": "apps/frontend/dist",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
