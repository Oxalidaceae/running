{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.2.29"
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/index.ts" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
